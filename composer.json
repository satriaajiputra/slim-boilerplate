{
    "name": "satmaxt/slim-boilerplate",
    "version": "4.3.0",
    "description": "Slim framework boilerplate with MVC mode",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "slim/slim": "^4.0",
        "slim/psr7": "^0.6.0",
        "selective/basepath": "^0.2.0",
        "php-di/slim-bridge": "^3.0",
        "illuminate/database": "^6.6",
        "slim/php-view": "^2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4" : {
            "App\\" : "app"
        }
    },
    "authors": [
        {
            "name": "Satria Aji Putra",
            "email": "satriamaxt@gmail.com"
        }
    ]
}
